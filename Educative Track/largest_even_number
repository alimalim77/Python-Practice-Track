inp = input("Enter the alphanumberic string")
mystr=""

for i in inp:
    if i.isdigit():
        mystr = mystr + i
s1 = set(mystr)
s2 = [int(x) for x in s1]
s2.sort(reverse=True)

for i in range(len(s2)-1,-1,-1):
    if i % 2 == 0:
        s2[i],s2[-1]=s2[-1],s2[i]
        break

s3 = list(map(str, s2)) 
s4 = "".join(s3)
print(s4)